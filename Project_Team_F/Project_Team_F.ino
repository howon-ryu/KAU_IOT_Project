// scaling function to convert from any range to any range
int scaling(int value, int rowMin, int rowMax, int newRange0, int newRange100 ) 
{
    int x = (((value - rowMin) * (newRange100 - newRange0)) / (rowMax - rowMin)) + newRange0;
    if (x < 0)
    { 
      return 0;
    }
    else if(x > 100)
    { 
      return 100; 
    }
    else
    { 
      return x; 
    }


}
